<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Beliberda</servlet-name>
		<servlet-class>beliberda.BeliberdaServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Blobstore</servlet-name>
		<servlet-class>account.BlobstoreUploadServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>History</servlet-name>
		<servlet-class>history.HistoryServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Test</servlet-name>
		<servlet-class>test.TestServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Tasker</servlet-name>
		<servlet-class>tasks.TaskerServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Profile</servlet-name>
		<servlet-class>account.ProfileServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Beliberda</servlet-name>
		<url-pattern>/beliberda</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Blobstore</servlet-name>
		<url-pattern>/blobstore</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>History</servlet-name>
		<url-pattern>/history</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/test</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Tasker</servlet-name>
		<url-pattern>/tasker</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Profile</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<filter>
        <filter-name>bestHistory</filter-name>
        <filter-class>filters.BestHistoryFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>bestHistory</filter-name>
        <url-pattern>/filters/bestHistory/*</url-pattern>
    </filter-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
	<error-page>
	    error.jsp
	</error-page>
	<security-constraint>
        <web-resource-collection>
            <url-pattern>/tasks/*</url-pattern>
        </web-resource-collection>
        <auth-constraint>
            <role-name>admin</role-name>
        </auth-constraint>
    </security-constraint>
</web-app>
